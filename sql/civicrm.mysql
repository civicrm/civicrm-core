-- * Table to store the reminders.
-- *
-- *******************************************************/
CREATE TABLE `civicrm_action_schedule` (


     `id` int unsigned NOT NULL AUTO_INCREMENT  ,
     `name` varchar(64)    COMMENT 'Name of the action(reminder)',
