     `acl_id` int unsigned    COMMENT 'ID of the ACL or ACL group being granted/revoked',
     `is_active` tinyint    COMMENT 'Is this property active?' 
,
    PRIMARY KEY ( `id` )
 
    ,     INDEX `index_acl_id`(
        acl_id
  )
  
 
)  ENGINE=InnoDB DEFAULT CHARACTER SET utf8 COLLATE utf8_unicode_ci  ;

