<?xml version="1.0"?>
<extension key="ckeditor4" type="module">
  <file>ckeditor4</file>
  <name>CKEditor</name>
  <description>CiviCRM CKEditor Plugin.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Seamus Lee</author>
    <email>seamus@civicrm.org</email>
  </maintainer>
  <urls>
