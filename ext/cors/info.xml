<?xml version="1.0"?>
<extension key="cors" type="module">
  <file>cors</file>
  <name>CORS</name>
  <description>Adds CORS headers to responses sent to requests for CiviCRM URLs.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Michael McAndrew</author>
    <email>michael@3sd.io</email>
  </maintainer>
  <urls>
    <url desc="Documentation">https://docs.civicrm.org/dev/en/latest/framework/cors/</url>
    <url desc="Support">https://chat.civicrm.org/civicrm/channels/dev</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-10-18</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.55</ver>
  </compatibility>
  <classloader>
    <psr0 prefix="CRM_" path="." />
    <psr4 prefix="Civi\" path="Civi" />
  </classloader>
  <civix>
    <namespace>CRM/Cors</namespace>
    <format>22.10.0</format>
    <angularModule>crmCors</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>scan-classes@1.0.0</mixin>
  </mixins>
</extension>
