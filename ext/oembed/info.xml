<?xml version="1.0"?>
<extension key="oembed" type="module">
  <file>oembed</file>
  <name>oEmbed</name>
  <description>Export CiviCRM content using the oEmbed protocol</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Tim Otten</author>
    <email>totten@civicrm.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-08-08</releaseDate>
  <version>5.78.alpha1</version>
  <develStage>alpha</develStage>
  <tags>
    <tag>mgmt:hidden</tag>
  </tags>
  <compatibility>
    <ver>5.78</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <requires>
    <ext>iframe</ext>
  </requires>
  <civix>
    <namespace>CRM/Oembed</namespace>
    <format>23.02.1</format>
    <angularModule>crmOembed</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>scan-classes@1.0.0</mixin>
    <mixin>setting-admin@1.0.1</mixin>
    <mixin>ang-php@1.0.0</mixin>
  </mixins>
</extension>
